(spawn "cmd.exe")
(let ((ctrlc (format nil "~A" (convert 3 <character>))))
  (block main
    (while t
      (case (expect* 'timeout 10 "xxx" "yyy")
        ((0)
         (send ctrlc)
         (sendln "exit")
         (return-from main nil)
         )
        ((1)
         (send ctrlc)
         (sendln "rem")
         )
        ((-1)
         (send ctrlc)
         (sendln "exit")
         (return-from main nil)
        )
        )
      )
    )
  )
